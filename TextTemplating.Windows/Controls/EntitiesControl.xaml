<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:dto="clr-namespace:kkkkkkaaaaaa.VisualStudio.TextTemplating.DataTransferObjects;assembly=TextTemplating" x:Class="TextTemplating.Windows.Controls.EntitiesControl" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid x:Name="GridEntities">
        <Label x:Name="LabelNamespace" HorizontalAlignment="Left" Margin="10,10,0,258" Content="Namespace" />
        <TextBox x:Name="TextNamespace" Text="{Binding Namespace, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" HorizontalAlignment="Left" Height="23" Margin="105,19,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Width="120" />
    </Grid>
</UserControl>
